{
  "name": "angular-intro",
  "version": "1.0.0",
  "private": true,
  "author": "IMS",
  "license": "MIT",
  "description": "An introduction to IMS specific libraries and patterns",
  "main": "public/index.html",
  "scripts": {
    "start": "parcel public/index.html --open",
    "build": "parcel build public/index.html",
    "test": "jest"
  },
  "dependencies": {
    "@uirouter/react-hybrid": "0.3.10",
    "angular": "1.8.0",
    "emotion": "10.0.9",
    "react": "16.8.6",
    "react-dom": "16.8.6"
  },
  "devDependencies": {
    "@babel/core": "7.4.3",
    "@babel/plugin-proposal-class-properties": "7.4.0",
    "@babel/preset-env": "7.4.3",
    "@babel/preset-react": "7.0.0",
    "@babel/preset-typescript": "7.3.3",
    "@types/angular": "1.6.54",
    "@types/angular-mocks": "1.7.0",
    "@types/jest": "24.0.11",
    "@types/react": "16.8.13",
    "@types/react-dom": "16.8.4",
    "angular-mocks": "1.5.11",
    "babel-jest": "24.7.1",
    "jest": "24.7.1",
    "parcel-bundler": "1.12.3",
    "typescript": "3.4.3"
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": {
            "node": "current"
          }
        }
      ],
      "@babel/preset-typescript",
      "@babel/preset-react"
    ],
    "plugins": [
      "@babel/plugin-proposal-class-properties"
    ]
  },
  "jest": {
    "setupFilesAfterEnv": [
      "./src/setupTests.js"
    ]
  }
}
